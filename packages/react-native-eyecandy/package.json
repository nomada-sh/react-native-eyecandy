{
  "name": "@nomada-sh/react-native-eyecandy",
  "version": "2.1.12",
  "description": "React Native Eyecandy",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "nomada-sh-react-native-eyecandy.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "build": "bob build",
    "prepare": "npm run build",
    "typecheck": "tsc --noEmit",
    "publish": "npm publish --access public",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "local:publish": "yalc publish --push"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/nomada-sh/react-native-eyecandy",
  "author": "nomada-sh (https://github.com/nomada-sh)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nomada-sh/react-native-eyecandy/issues"
  },
  "homepage": "https://github.com/nomada-sh/react-native-eyecandy#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "devDependencies": {
    "@types/color": "^3.0.2",
    "@types/jest": "^26.0.0",
    "@types/react": "*",
    "@types/react-native": "*",
    "jest": "^26.0.1",
    "react": "*",
    "react-native": "*",
    "react-native-builder-bob": "^0.18.2",
    "react-native-gesture-handler": "^2.2.0",
    "react-native-haptic-feedback": "^1.13.0",
    "react-native-image-picker": "^4.4.2",
    "react-native-reanimated": "2.3.1"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-gesture-handler": "^2.2.0",
    "react-native-haptic-feedback": "^1.13.0",
    "react-native-image-picker": "^4.4.2",
    "react-native-reanimated": "^2.3.1"
  },
  "dependencies": {
    "@nomada-sh/react-native-eyecandy-icons": "^1.0.4",
    "@nomada-sh/react-native-eyecandy-theme": "^1.0.1",
    "calendar-base": "^1.1.1",
    "color": "^4.2.0",
    "date-fns": "^2.28.0",
    "react-native-country-picker-modal": "^2.0.0",
