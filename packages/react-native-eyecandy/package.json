{
  "name": "@nomada-sh/react-native-eyecandy",
  "version": "2.0.0",
  "description": "React Native Eyecandy",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "nomada-sh-react-native-eyecandy.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "build": "bob build",
    "prepare": "npm run build",
    "typecheck": "tsc --noEmit",
    "local:publish": "yalc publish --push",
    "local:add": "yalc add --dev @nomada-sh/react-native-eyecandy-theme @nomada-sh/react-native-eyecandy-icons",
    "local:remove": "yalc remove --all",
    "local:check": "yalc check"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/nomada-sh/react-native-eyecandy",
  "author": "nomada-sh (https://github.com/nomada-sh)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nomada-sh/react-native-eyecandy/issues"
  },
  "homepage": "https://github.com/nomada-sh/react-native-eyecandy#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "devDependencies": {
    "@nomada-sh/react-native-eyecandy-icons": "file:.yalc/@nomada-sh/react-native-eyecandy-icons",
    "@nomada-sh/react-native-eyecandy-theme": "file:.yalc/@nomada-sh/react-native-eyecandy-theme",
    "@types/color": "^3.0.2",
    "@types/jest": "^26.0.0",
    "@types/react": "*",
    "@types/react-native": "*",
    "jest": "^26.0.1",
    "react": "*",
    "react-native": "*",
    "react-native-builder-bob": "^0.18.2",
    "react-native-gesture-handler": "*",
    "react-native-image-picker": "*",
    "react-native-reanimated": "*",
    "typescript": "^4.1.3",
    "yalc": "^1.0.0-pre.53"
  },
  "peerDependencies": {
    "@nomada-sh/react-native-eyecandy-icons": "*",
    "@nomada-sh/react-native-eyecandy-theme": "*",
    "react": "*",
    "react-native": "*",
    "react-native-gesture-handler": "*",
    "react-native-image-picker": "*",
    "react-native-reanimated": "*"
  },
  "dependencies": {
    "calendar-base": "^1.1.1",
    "color": "^4.2.0",
    "date-fns": "^2.28.0",
    "react-native-picker-select": "^8.0.4",
    "react-use": "^17.3.2",
    "rn-swipe-button": "^1.3.6"
  }
}
